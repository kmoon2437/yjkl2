{
    info:{
        title:'제목',
        subtitle:'부제목 또는 곡 자체의 변형(리믹스 ver 등) 관련 정보',
        relatedInfo:'ost/tie-in 정보',

        /**
         * 성별은 M=남자,F=여자,X또는 C=혼성
         * 뒤에 숫자는 도(C)=1부터 시작해서
         * 도#(C#),레b(Db),레(D),레#(D#),미b(Eb),
         * 미(E),파(F),파#(F#),솔b(Gb),... 이렇게 1씩 증가
         * 단조(minor)는 음수로 바꾸면 됨(eg. -11 = Gm)
         */
        key:['성별',1],

        originalKey:['...'], // key랑 같음(없어도 됨)
        singer:'가수',
        lyrics:'작사자', // 없어도 됨
        music:'작곡자', // 없어도 됨
        origin:'원곡이 있는 곡의 원곡 정보', // 없어도 됨
        tags:{
            singer:'가수태그',
            lyrics:'작사자태그',
            music:'작곡자태그',
            origin:'원곡정보태그'
        }
    },
    files:{
        // 얘네 둘 중 최소 하나는 무조건 있어야 됨
        midi:'반주.mid',
        audio:'반주.ogg',

        // 얘네는 있어도 되고 없어도 됨
        mv:'뮤비영상파일',
        album:'앨범사진파일'
    },
    config:{
        mvTiming:0, // 뮤비 시작타이밍(밀리초 단위)
        medleyInfo:null, // 메들리곡용 정보(미구현)
        
        /**
         * bit field 형식
         * 1 = 밑에 나오는 발음표기 표시
         * 2 = 루비(일본어 후리가나용) 표시
         * 4 = 다국어용 고딕 폰트(Noto sans라던가) 사용
         * 얘네 중에 쓸 거 다 더한 값
         */
        layout:0, // 없을 시 기본값 0

        // 가사 싱크(없을 시 기본값 0)
        syncOffset:0,

        // audio파일 쓸 때 가사 싱크(없을 시 기본값 0)
        audioSyncOffset:0,

        // 한 박자당 몇 틱으로 할 건지 설정(없을 시 기본값 120)
        ticksPerBeat:120
    },
    
    // bpm 지정 관련(없을시 기본값 120)
    tempo:[
        // changeAt = 이 템포로 바뀔 시간(틱 단위)
        // bpm = 그냥 bpm
        // 중간에 바꾸려면 걍 여러 개 넣으면 됨
        { changeAt:0,bpm:196 },
        { changeAt:1200,bpm:138 }
    ],
    lyrics:[
        {
            line:1, // 줄 위치(0 = 위,1 = 중간,2 = 아래)
            show:6000, // 표시시각
            hide:8000, // 숨김시각(없으면 다음에 같은 위치에 오는 가사 시작시간으로 적용)
            sub:'발음',
            start:5860,
            // startCount는 시작 카운트 넣는 거고 align은 글자 정렬(둘 다 없어도 됨)
            // style(가사 색)은 -1,0(기본),1,2,3,4가 있음
            params:{ startCount:true,style:0,align:0 },
            data: [
                ['가사',20],
                ['[내용]<후리가나>',20], // 대괄호를 써야 여러 글자로 루비를 넣을 수 있음
                [' ',0], // 띄어쓰기는 이렇게 하면 됨
                ['글<후리가나>',20], // 대괄호를 써야 여러 글자로 루비를 넣을 수 있음
                ['b'60],
                ['lu',60],
                ['e:<ブルー>',60],
                [' ',0],
                ['g',60],
                ['ra',60],
                ['d',120],
                ['a',60],
                ['tion:<グラデイション>',300], // 이렇게 영단어의 마지막 글자 뒤에 :를 붙이고 ruby를 넣으면 단어 하나를 묶어서 루비가 들어감
                ['光<ひかり>',20,40,20], // 타이밍을 여러 개 넣으면 그만큼 글자를 나눠 색칠함
                ['銃<じゅう>',[2,20],40], // 2:1 비율로 나눠 색칠
            ]
        }
    ]
}
/**
 * 영단어 루비 넣을 때 묶는 기준(묶이는 글자들)
 * 1. 영어 알파벳(a-z,A-Z)
 * 2. 축약형에 쓰이는 '(apostrophe)
 * (3부터는 영어가 아니지만)
 * ((숫자 제외) 글자 하나가)
 * (음절이 아닌 음소 단위이므로)
 * (영어 알파벳과 동일하게 취급)
 * 3. 숫자
 * 4. 라틴 문자(알파벳 위에 뭐 점같은거 붙어있는거, 기타등등)
 * 5. 키릴 문자
 * 6. 그리스 문자
 * (추후 추가될 수 있음)
*/
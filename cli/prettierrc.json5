// json5 변환프로그램용
{
    "parser":"json5",
    "arrowParens":"always",
    "bracketSameLine":false,
    "bracketSpacing":true,
    "embeddedLanguageFormatting":"auto",
    "htmlWhitespaceSensitivity":"css",
    "insertPragma":false,
    "jsxSingleQuote":false,
    "printWidth":100,
    "proseWrap":"preserve",
    "quoteProps":"as-needed",
    "requirePragma":false,
    "semi":true,
    "singleAttributePerLine":false,
    "singleQuote":true,
    "tabWidth":4,
    "trailingComma":"es5",
    "useTabs":false,
    "vueIndentScriptAndStyle":false
}
